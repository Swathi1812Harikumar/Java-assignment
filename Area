import java.util.*;
public class Area
{
    static void findArea(int a)
    {
        int square=a*a;
        System.out.println("Area of a square : "+square);
    }
    static void findArea(int l,int b)
    {
        int rectangle=l*b;
        System.out.println("Area of a rectangle : "+rectangle);
    }
    static void findArea(int r,double pi)
    {
        double circle=pi*r*r;
        System.out.println("Area of a circle : "+circle);
    }
    
    
    
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int l=s.nextInt();
        int b=s.nextInt();
        int r=s.nextInt();
        double pi=3.14;
        findArea(a);
        findArea(l,b);
        findArea(r,pi);
    }
}
