import java.util.*;
public class Arithmetic{
    public static void main(String[] args) {
   try {
         Scanner s=new Scanner(System.in);
         int a=s.nextInt();
         int b=0;
         int c = a/b;
            System.out.println (c);
       }
       catch(ArithmeticException e) {
            System.out.println ("Can't divide a number by 0");
        }
    }
}